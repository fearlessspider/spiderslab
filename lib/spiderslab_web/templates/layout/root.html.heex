              <li><%= link "LiveDashboard", to: Routes.live_dashboard_path(@conn, :home) %></li>
            <% end %>
          </ul>
        </nav>
        <a href="/" class="logo">
          <h1>Spider's LAB</h1>
        </a>
      </section>
    </header>
    <%= @inner_content %>
    <footer>
      <div class="container">
        <section class="row">
          <article class="column">
            <h2>Resources</h2>
            <ul>
              <li>
              </li>
            </ul>
          </article>
          <article class="column">
            <h2>Help</h2>
            <ul>
              <li>
              </li>
            </ul>
          </article>
          <article class="column">
            <h2>Contact</h2>
